{
  "headers": [
    {
      "source": "/api/auth/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, no-cache, must-revalidate"
        },
        { "key": "Pragma", "value": "no-cache" },
        { "key": "Expires", "value": "0" }
      ]
    }
  ],
  "functions": {
    "app/api/auth/[...nextauth]/route.ts": {
      "maxDuration": 30
    },
    "app/editor/page.tsx": {
      "maxDuration": 30
    }
  },
  "env": {
    "PRISMA_QUERY_ENGINE_LIBRARY": "1"
  },
  "build": {
    "env": {
      "PRISMA_QUERY_ENGINE_LIBRARY": "1"
    }
  }
}
